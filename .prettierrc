{
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 240,
    "importOrder": [
        "^react",
        "^@?\\w",
        "^@/",
        "^\\.\\.?/"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true,
    "importOrderParserPlugins": [
        "typescript",
        "jsx"
    ],
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ]
}